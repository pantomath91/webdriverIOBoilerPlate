{
  "name": "netspend-testHarnessApp",
  "version": "1.0.0",
  "description": "Framework to run WebdriverIO tests with Appium to perform UI/Integration testing of hybrid applications of netspend apps on iOS and Android",
  "scripts": {
    "android.build.app": "cd testHarnessApps/android/testapp && ./gradlew clean build && cd - && cp testHarnessApps/android/testapp/app/build/outputs/apk/debug/app-debug.apk apps",
    "android.browserstack.app": "./node_modules/.bin/wdio ./config/browserstack/wdio.android.bs.app.conf.js",
    "android.app": "./node_modules/.bin/wdio ./config/wdio.android.app.conf.js",
    "android.browser": "./node_modules/.bin/wdio ./config/wdio.android.browser.conf.js",
    "ios.build.app": "cd testHarnessApps/ios && bundle exec fastlane build_framework",
    "ios.app": "./node_modules/.bin/wdio ./config/wdio.ios.app.conf.js",
    "ios.browserstack.app": "./node_modules/.bin/wdio ./config/browserstack/wdio.ios.bs.app.conf.js",
    "ios.browser": "./node_modules/.bin/wdio ./config/wdio.ios.browser.conf.js",
    "lint": "eslint config tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/appium-boilerplate.git"
  },
  "keywords": [
    "WebdriverIO",
    "WebDriver",
    "appium",
    "native app",
    "hybrid",
    "android",
    "ios"
  ],
  "author": "slohani@netspend.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@babel/traverse": "^7.10.4",
    "@babel/types": "^7.10.4",
    "@wdio/appium-service": "^6.1.0",
    "@wdio/browserstack-service": "^6.12.1",
    "@wdio/cli": "^6.1.12",
    "@wdio/cucumber-framework": "^6.1.13",
    "@wdio/jasmine-framework": "^6.11.0",
    "@wdio/local-runner": "^6.1.12",
    "@wdio/mocha-framework": "^6.1.8",
    "@wdio/sauce-service": "^6.12.1",
    "@wdio/spec-reporter": "^6.1.12",
    "@wdio/sync": "^6.1.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-wdio": "^6.6.0",
    "node-fetch": "^2.6.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@wdio/allure-reporter": "^5.0.0",
    "allure-commandline": "^2.13.8",
    "multiple-cucumber-html-reporter": "^1.16.2",
    "wdio-cucumberjs-json-reporter": "^2.0.2",
    "wdio-video-reporter": "^3.1.1"
  }
}
